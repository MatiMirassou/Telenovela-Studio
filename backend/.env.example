# Gemini API Key (optional if users provide their own via Settings UI)
GEMINI_API_KEY=your-gemini-api-key-here

# Database URL (default: SQLite in current directory)
DATABASE_URL=sqlite:///telenovela.db

# CORS Origins (comma-separated, for production set your domain)
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# App Password (optional - leave empty to disable auth, set to enable login screen)
APP_PASSWORD=
